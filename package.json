{
  "name": "pasteportal",
  "displayName": "PastePortal",
  "author": "stiliajohny",
  "license": "GPL-3.0",
  "homepage": "https://pasteportal.info",
  "publisher": "JohnStilia",
  "description": "A text sharing tool for developers",
  "icon": "pasteportal_icon.png",
  "version": "0.1.8",
  "preview": true,
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "paste",
    "portal",
    "share",
    "text",
    "code",
    "snippet",
    "pasteportal"
  ],
  "contributors": [
    {
      "name": "stiliajohny",
      "url": "https://github.com/stiliajohny"
    },
    {
      "name": "cur5or",
      "url": "https://github.com/Cur50r"
    },
    {
      "name": "DanutEne",
      "url": "https://github.com/DanutEne"
    }
  ],

  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pasteportal.get-paste",
        "title": "PastePortal: Get Paste"
      },
      {
        "command": "pasteportal.store-paste",
        "title": "PastePortal: Store Paste"
      }
    ],
    "keybindings": [
      {
        "command": "pasteportal.get-paste",
        "mac": "ctrl+cmd+alt+v",
        "key": "ctrl+cmd+alt+v",
        "when": "editorTextFocus"
      },
      {
        "command": "pasteportal.store-paste",
        "mac": "ctrl+cmd+alt+c",
        "key": "ctrl+cmd+alt+c",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "properties": {
        "pasteportal.serviceAgreementAccepted": {
          "type": "boolean",
          "default": false
        }
      }
    }
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/stiliajohny/vscode-ext-pasteportal.git"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "axios": "^0.24.0"
  }
}